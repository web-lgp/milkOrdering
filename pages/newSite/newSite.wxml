
<view style="margin-top: 20px;height: 100vh;background-color: #f7f8fa;">
  <van-cell-group>
    <van-field
      model:value="{{ username }}"
      label="收货人"
      placeholder="请输入"
    />
    <van-field
      model:value="{{ phone }}"
      label="手机号"
      placeholder="请输入"
    />
    <van-field
      model:value="{{ address }}"
      label="收货地址"
      placeholder="请输入"
    />
    <van-field
      model:value="{{ name }}"
      label="详细地址"
      type="textarea"
      placeholder="请输入"
    />
  </van-cell-group>
  <view class="queren" bindtap="onQueRen">确认</view>
</view>
